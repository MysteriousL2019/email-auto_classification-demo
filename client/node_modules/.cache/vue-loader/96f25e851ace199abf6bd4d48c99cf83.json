n\n      if (res.data.flag==\"1\")\n      {\n          this.$message.success(\"查询成功\",0.3);\n      } else {\n          this.$message.error(\"查询失败！！！\",0.3);\n        }\n\n\n        })\n        .catch((error) => {\n          // eslint-disable-next-line\n          console.error(error);\n        });\n \n    },\n\n  },\n  created() {\n    // this.predict();\n  },\n\n\n\n  \n}\n\n\n\n</script>\n"]}]}